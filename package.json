{
  "name": "threejs_boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "prod": "cross-env NODE_ENV=production vite",
    "build": "gulp dev && npm run copyfiles",
    "preview": "vite preview",
    "copyfiles": "cp -r .env* certs/ public migrations ./dist/",
    "prebuild": "rm -rf dist/",
    "build2": "tsc -p tsconfig.json && npm run copyfiles",
    "preinstall": "npx npm-force-resolutions",
    "dev:wsl": "cross-env NODE_ENV=development CHOKIDAR_USEPOLLING=true nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.test.ts\" --exec \"ts-node-esm --esm --files src/index.ts\"",
    "start": "cross-env NODE_ENV=production node build/src/index.js",
    "deptree3": "depcruise --exclude \"^(node_modules|forks|__tests__)\" -T dot src/ | dot -T svg > dependenciesGraph.svg",
    "depcollapse": "depcruise --exclude node_modules/ -c \".dependency-cruiser-collapse.js\" -T dot src/ | dot -T svg > deptree.svg",
    "deptree": "depcruise --exclude node_modules/ -c \".dependency-cruiser.js\" -T dot src/ | dot -T svg > dependenciesGraph.svg"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/register": "^7.21.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/gulp": "^4.0.11",
    "@types/gulp-typescript": "^2.13.0",
    "@types/jest": "^29.5.2",
    "@types/three": "^0.152.1",
    "@types/uuid": "^9.0.1",
    "browserify": "^17.0.0",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^13.0.2",
    "dotenv": "^16.1.3",
    "esm": "^3.2.25",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "jest": "^29.5.0",
    "sass": "^1.62.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsify": "^5.0.4",
    "typescript": "^5.0.2",
    "uglifyjs": "^2.4.11",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "three": "^0.153.0",
    "uuid": "^9.0.0"
  }
}
